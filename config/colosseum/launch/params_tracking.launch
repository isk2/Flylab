<launch>
  <!-- Some example command-line things you can do...
       rosparam get tracking/diff_threshold
       rosparam set tracking/diff_threshold 18
  -->


  <param name="tracking/diff_threshold"     type="double" value="16"/>   					<!-- Adjust this based on light brightness, etc, to get good contours. -->
  <param name="tracking/dtForecast"         type="double" value="0.20"/>   					<!-- Adjust this based on light brightness, etc, to get good contours. -->
  <param name="tracking/alphaBackground"    type="double" value="0.000001"/>   				<!-- Moving average for dynamic background. -->
  <param name="tracking/filenameBackground" type="string" value="/cameras/background.png"/> <!-- Location of the initial background image. -->

</launch>
