<launch>
	<!-- ========================================================== -->
	<!-- Configuration of the ROBOT (i.e. the "magnet")             -->
    <!-- Don't launch this file directly.                           -->
    <!-- Launch hardware.launch, and point to this file from there. -->
    <!--                                                            -->
    <!-- Edit this file to describe the particular robot.           -->
	<!-- ========================================================== -->


	<param name="robot_width" type="double" value="1.5875"/>
	<param name="robot_height" type="double" value="1.5875"/>
	<param name="robot_visible" type="boolean" value="true"/>
	<param name="robot_paint" type="str" value="blackoxide"/>
	<param name="robot_scent" type="str" value="unscented"/>
  
	<include file="$(find robots)/launch/calibration_robotimage_0625inch.launch" />
  
</launch>
  
