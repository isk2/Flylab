<launch>
	<!-- ========================================================== -->
	<!-- FLYCORE:  Required Nodes for Flyatar2.                     -->
    <!--                                                            -->
    <!-- Don't launch this file directly.                           -->
    <!-- Include this file in your main launch file.                -->
    <!--                                                            -->
	<!-- ========================================================== -->
  
	<include file="$(find image_gui)/launch/DisplayDraw.launch" />
